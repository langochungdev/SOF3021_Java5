package controller;
import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class Ok {
    @RequestMapping("/ok")
    public String ok(Model m) {
        m.addAttribute("noidung", "ok");
        return "ok";
    }

    @RequestMapping("/ok1")
    public String m1(Model model) {
        m.addAttribute("noidung", "m1");
        return "ok";
    }

    @RequestMapping("/ok2")
    public String m2(Model model) {
        m.addAttribute("noidung", "m2");
        return "ok";
    }

    @RequestMapping("/ok3")
    public String m3(Model model) {
        m.addAttribute("noidung", "m3");
        return "ok";
    }
}

